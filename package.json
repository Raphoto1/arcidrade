{
  "name": "arcidrade",
  "version": "0.9.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "test:database": "tsx scripts/test-database.ts",
    "clean": "rm -rf node_modules package-lock.json .next .turbo",
    "clean:win": "rmdir /s /q node_modules & del package-lock.json & rmdir /s /q .next & rmdir /s /q .turbo",
    "sync": "npm run clean && npm install && npm run build",
    "sync:win": "npm run clean:win && npm install && npm run build",
    "fresh": "npm run sync && npm run dev",
    "fresh:win": "npm run sync:win && npm run dev"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@next/third-parties": "^16.0.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/extension-accelerate": "^3.0.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^2.0.1",
    "@vercel/speed-insights": "^1.2.0",
    "bcrypt": "^6.0.0",
    "cally": "^0.8.0",
    "country-state-city": "^3.2.1",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "isomorphic-dompurify": "^2.36.0",
    "next": "^15.5.11",
    "next-auth": "^4.24.11",
    "nodemailer": "^7.0.11",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "quill": "^2.0.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "swr": "^2.3.6",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@prisma/client": "^7.3.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.21",
    "@types/papaparse": "^5.3.16",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.0.50",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "postcss": "^8.5.6",
    "prisma": "^7.3.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5"
  }
}
